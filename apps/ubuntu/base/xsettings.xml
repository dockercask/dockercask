<?xml version="1.0" encoding="UTF-8"?>

<channel name="xsettings" version="1.0">
  <property name="Net" type="empty">
    <property name="ThemeName" type="string" value="{{ gtk2_theme }}"/>
    <property name="IconThemeName" type="string" value="{{ gtk2_icon_theme }}"/>
  </property>
  <property name="Xft" type="empty">
    <property name="DPI" type="int" value="{{ dpi }}"/>
  </property>
  <property name="Gtk" type="empty">
    <property name="FontName" type="string" value="{{ gtk2_font }}"/>
    <property name="CursorThemeName" type="string" value="{{ gtk2_cursor_theme }}"/>
  </property>
  <property name="Xfce" type="empty">
    <property name="LastCustomDPI" type="int" value="{{ dpi }}"/>
  </property>
</channel>
